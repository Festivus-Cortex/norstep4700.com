{
  "audioAnalysis": {
    "fft": {
      "fftSize": 1024,
      "frequencyBinCount": 512
    },
    "frequencyBands": {
      "bass": {
        "start": 0,
        "end": 10
      },
      "midLow": {
        "start": 11,
        "end": 40
      },
      "midHigh": {
        "start": 41,
        "end": 80
      },
      "treble": {
        "start": 81,
        "end": 511
      }
    },
    "normalization": {
      "rmsMultiplier": 2,
      "frequencyDivisor": 255
    }
  },
  "intensityMultipliers": {
    "subtle": 0.2,
    "mild": 0.4,
    "moderate": 0.6,
    "strong": 0.8,
    "extreme": 1.2
  },
  "effects": {
    "maskRadiusAnimator": {
      "defaults": {
        "intensity": "strong",
        "audioAnalysisSource": "bass",
        "baseRadius": 75,
        "minRadius": 35,
        "maxRadius": 110,
        "smoothing": 0.7
      },
      "intensityOverrides": {
        "subtle": {
          "smoothing": 0.85
        },
        "extreme": {
          "smoothing": 0.4
        }
      },
      "variants": {
        "trackVisualizer": {
          "intensity": "strong",
          "audioAnalysisSource": "bass",
          "baseRadius": 50,
          "minRadius": 9,
          "maxRadius": 90,
          "smoothing": 0.2
        }
      }
    },
    "gradientTiltAnimator": {
      "defaults": {
        "intensity": "extreme",
        "audioAnalysisSource": "rms",
        "baseTilt": 0,
        "minTilt": -180,
        "maxTilt": 180,
        "smoothing": 0.3,
        "oscillate": true
      },
      "intensityOverrides": {
        "subtle": {
          "maxTilt": 20,
          "minTilt": -20,
          "smoothing": 0.85
        },
        "extreme": {
          "maxTilt": 90,
          "minTilt": -90,
          "smoothing": 0.4
        }
      }
    },
    "dotsOpacityAnimator": {
      "defaults": {
        "intensity": "extreme",
        "audioAnalysisSource": "midHigh",
        "baseOpacity": 60,
        "minOpacity": 25,
        "maxOpacity": 100,
        "smoothing": 0.2
      },
      "intensityOverrides": {
        "subtle": {
          "minOpacity": 30,
          "maxOpacity": 70,
          "smoothing": 0.7
        },
        "extreme": {
          "minOpacity": 25,
          "maxOpacity": 100,
          "smoothing": 0.1
        }
      }
    },
    "gradientScaleAnimator": {
      "defaults": {
        "intensity": "extreme",
        "audioAnalysisSource": "midLow",
        "baseWidth": 25,
        "baseHeight": 25,
        "minScale": 0.3,
        "maxScale": 4.0,
        "smoothing": 0.2,
        "aspectLock": false
      },
      "intensityOverrides": {
        "subtle": {
          "minScale": 0.8,
          "maxScale": 1.3,
          "smoothing": 0.8
        },
        "extreme": {
          "minScale": 0.3,
          "maxScale": 3.0,
          "smoothing": 0.4
        }
      }
    },
    "glitchIntensityAnimator": {
      "defaults": {
        "intensity": "strong",
        "audioAnalysisSource": "treble",
        "threshold": 0.3,
        "slowThreshold": 0.4,
        "mediumThreshold": 0.7,
        "smoothing": 0.3,
        "holdTime": 100
      },
      "intensityOverrides": {
        "subtle": {
          "threshold": 0.6,
          "holdTime": 50
        },
        "extreme": {
          "threshold": 0.15,
          "holdTime": 200
        }
      }
    },
    "gradientPositionAnimator": {
      "defaults": {
        "intensity": "extreme",
        "audioAnalysisSource": "rms",
        "baseX": 50,
        "baseY": 50,
        "maxDeviation": 40,
        "smoothing": 0.2,
        "peakThreshold": 0.1,
        "peakDecay": 0.92,
        "movementStyle": "radial"
      },
      "intensityOverrides": {
        "subtle": {
          "maxDeviation": 8,
          "peakThreshold": 0.25,
          "smoothing": 0.6
        },
        "extreme": {
          "maxDeviation": 25,
          "peakThreshold": 0.1,
          "smoothing": 0.2
        }
      }
    }
  }
}
